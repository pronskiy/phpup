{
  "name": "pronskiy/phpup",
  "description": "The PHP toolchain installer",
  "keywords": [
    "php",
    "toolchain",
    "composer"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^8.3",
    "composer/composer": "^2.7.3",
    "symfony/var-dumper": "^v7.0.6",
    "cweagans/composer-patches": "^2.0.0-beta2",
    "pronskiy/locus": "^0.2",
    "artisan-build/conductor": "^0.2.0",
    "symfony/console": "^7.0.6"
  },
  "extra": {
    "patches": {
      "symfony/var-dumper": {
        "Fix SAPI": "patches/symfony/var-dumper.patch"
      },
      "composer/composer": {
        "Fix SAPI": "patches/composer/composer.patch"
      },
      "artisan-build/conductor": {
        "Update composer bin path": "patches/update-composer-bin-path.patch"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "pronskiy/locus": false,
      "bamarni/composer-bin-plugin": true
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "dev-master"
  }
}
